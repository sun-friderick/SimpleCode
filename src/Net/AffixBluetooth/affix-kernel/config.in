#

# Affix version
AFFIX_VERSION=3.2.0

# normal
LINUX=/usr/src/linux
DESTDIR=
MODDIR=
ARCH=

# cross compiler stuff
#LINUX=/usr/local/arm/linux
#DESTDIR=/usr/local/arm/2.95.3
CROSS_COMPILE=/usr/local/arm/2.95.3/bin/arm-linux-
CROSS_LDFLAGS=
CROSS_CPPFLAGS=
CC=gcc
LD=ld

# Affix config options
CONFIG_AFFIX_DEBUG=n
CONFIG_AFFIX_DBMASK=0x00000000
#CONFIG_AFFIX_DBMASK=0xFFFFFFFF

# General/Protocols
CONFIG_AFFIX_SCO=y
CONFIG_AFFIX_L2CAP=y
CONFIG_AFFIX_HCI_BROADCAST=n
CONFIG_AFFIX_L2CAP_GROUPS=n
CONFIG_AFFIX_UPDATE_CLOCKOFFSET=n
CONFIG_AFFIX_RFCOMM_MODULE=y
CONFIG_AFFIX_PAN_MODULE=y
CONFIG_AFFIX_HIDP_MODULE=y
CONFIG_AFFIX_BT_1_2=y


# Drivers

# USB
CONFIG_AFFIX_USB_MODULE=y

# UART
CONFIG_AFFIX_UART_MODULE=y
CONFIG_AFFIX_UART_TLP=y
CONFIG_AFFIX_UART_H4=y
CONFIG_AFFIX_UART_BCSP=y
CONFIG_AFFIX_UART_NEW_BH=n

# PCMCIA
CONFIG_AFFIX_EXT_PCMCIA=n
CONFIG_AFFIX_UART_CS_MODULE=y
CONFIG_AFFIX_DTL1=y
CONFIG_AFFIX_DTL1_FIX=n
CONFIG_AFFIX_BLUECARD_CS_MODULE=y
CONFIG_AFFIX_BT3C_CS_MODULE=y
CONFIG_AFFIX_BT950UART_CS_MODULE=y

